{
	"manifest_version": 3,
	"name": "alx-to-google-calender",
	"description": "this extension will save the alx schedules to google calender",
	"version": "0.1.0",
	"permissions": ["tabs"],
	"host_permissions": ["https://*intranet.alxswe.com/planning/me"],
	"background": {
		"service_worker": "background.js"
	  },
	  "content_scripts": [
		{
		  "matches": ["https://*intranet.alxswe.com/planning/me"],
		  "js": ["contentScript.js"]
		}
	  ],
	"action": {
		"default_icon": {
		  "16": "img/icon.png",
		  "24": "img/icon.png",
		  "32": "img/icon.png"
		},
		"default_title": "alx-to-google",
		"default_popup": "popup.html"
	  }
}
